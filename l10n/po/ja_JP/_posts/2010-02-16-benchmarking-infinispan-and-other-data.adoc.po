#. type: delimited block -
#: upstream/_posts/2010-02-16-benchmarking-infinispan-and-other-data.adoc:52
#, no-wrap
msgid "<bench-config>\n"
msgstr ""

#. type: delimited block -
#: upstream/_posts/2010-02-16-benchmarking-infinispan-and-other-data.adoc:54
#, no-wrap
msgid "  <master bindAddress=\"${127.0.0.1:master.address}\" port=\"${2103:master.port}\"/>\n"
msgstr ""

#. type: delimited block -
#: upstream/_posts/2010-02-16-benchmarking-infinispan-and-other-data.adoc:63
#, no-wrap
msgid ""
"  <benchmark initSize=\"2\" maxSize=\"${4:slaves}\" increment=\"1\">\n"
"    <DestroyWrapper runOnAllSlaves=\"true\"/>\n"
"    <StartCluster/>\n"
"    <ClusterValidation partialReplication=\"false\"/>\n"
"    <Warmup operationCount=\"1000\"/>\n"
"    <WebSessionBenchmark numberOfRequests=\"2500\" numOfThreads=\"2\"/>\n"
"    <CsvReportGeneration/>\n"
"  </benchmark>\n"
msgstr ""

#. type: delimited block -
#: upstream/_posts/2010-02-16-benchmarking-infinispan-and-other-data.adoc:74
#, no-wrap
msgid ""
