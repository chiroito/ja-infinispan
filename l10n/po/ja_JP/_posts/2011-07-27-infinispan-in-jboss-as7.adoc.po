#: upstream/_posts/2011-07-27-infinispan-in-jboss-as7.adoc:117
msgid "#<subsystem xmlns=\"urn:jboss:domain:jgroups:1.0\" default-stack=\"udp\">#"
msgstr ""

#. type: delimited block _
#: upstream/_posts/2011-07-27-infinispan-in-jboss-as7.adoc:119
msgid "#  <stack name=\"udp\">#"
msgstr ""

#. type: delimited block _
#: upstream/_posts/2011-07-27-infinispan-in-jboss-as7.adoc:121
#: upstream/_posts/2011-07-27-infinispan-in-jboss-as7.adoc:127
msgid "#    <!-- ... -->#"
msgstr ""

#. type: delimited block _
#: upstream/_posts/2011-07-27-infinispan-in-jboss-as7.adoc:123
