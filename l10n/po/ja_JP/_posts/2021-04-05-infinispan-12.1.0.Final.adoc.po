msgstr ""

#. type: Plain text
#: upstream/_posts/2021-04-05-infinispan-12.1.0.Final.adoc:67
msgid "Grafana Dashboard"
msgstr ""

#. type: Plain text
#: upstream/_posts/2021-04-05-infinispan-12.1.0.Final.adoc:68
msgid "Batch CR"
msgstr ""

#. type: Plain text
#: upstream/_posts/2021-04-05-infinispan-12.1.0.Final.adoc:69
msgid "Custom library support"
msgstr ""

#. type: Plain text
#: upstream/_posts/2021-04-05-infinispan-12.1.0.Final.adoc:70
msgid "Custom labels on Resources created by Operator"
msgstr ""

